5.1) select min(sal) min_sal, max(sal) max_sal, avg(sal) avg_sal, sum(sal) total_sal from emp1;
5.2)select prj_name from proj1 where dno in (select dno from dept1 where dname='Marketing');
5.3)select upper(ename) uppercase,lower(ename) lowercase,initcap(ename) capitalized, length(ename) len, substr(ename,2,3) substring from emp1;
5.4)select * from emp1 where mgrid =(select empno from emp1 where ename='Raghu');
5.5)select dname, max(sal),min(sal) from dept1 d,emp1 e where d.dno=e.deptno group by d.dname;
5.6)select count(ename),dname from dept1 d,emp1 e where d.dno=e.deptno group by dname;
5.7)select * from emp1 where current_date - date_join > 1825;
5.8)select * from emp1 where date_birth between '01-dec-2000' and '31-dec-2000';
5.9)select count(ename),dname from dept1 d,emp1 e where d.dno=e.deptno group by dname having count(ename)>3;
5.10) select * from emp1 e,proj1 p where e.prj_id=p.prj_no and prj_name='Drug Repositioning';
5.11)
5.12)  select * from proj1 where end_date - strt_date >365;
5.13)
5.14)select * from dept1 where dno = (select dno from proj1 where prj_no is null);
